@model List<WishListViewModel>
@{
    ViewData["Title"] = "My Wishlist";
}

<h2 class="text-center">@ViewData["Title"]</h2>
<hr/>

@if (!Model.Any())
{
    <h2 class="text-center">You do not have any book yet!</h2>
}
else
{
    <div class="row">
        @foreach (WishListViewModel book in Model)
        {
            <partial name="_WishlistPartial" model="book" />
        }
    </div>
}
